{
  "name": "npm-starter",
  "version": "1.0.0",
  "main": "build/main.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/servexyz/npm-starter"
  },
  "devDependencies": {
    "@ava/babel-preset-stage-4": "^1.1.0",
    "@babel/core": "^7.0.0-beta.49",
    "@babel/node": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/register": "^7.0.0-beta.49",
    "ava": "^1.0.0-beta.5.1",
    "babel-loader": "^8.0.0-beta.3",
    "babel-plugin-dotenv": "^0.1.1",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "idempotent-babel-polyfill": "^0.1.1",
    "nodemon-webpack-plugin": "^4.0.3",
    "npm-run-all": "^4.1.3",
    "source-map-loader": "^0.2.3",
    "webpack": "^4.11.1",
    "webpack-cli": "^3.0.2",
    "webpack-node-externals": "^1.7.2"
  },
  "scripts": {
    "dev": " npm run start:webpack",
    "start:babel": "babel-node src/index.js",
    "start:webpack": "webpack --mode development --watch",
    "start": "npm-run-all -s clean build test",
    "clean": "rm -Rf ./build",
    "build": "webpack --mode production",
    "test": "ava"
  },
  "ava": {
    "require": [
      "@babel/register",
      "idempotent-babel-polyfill"
    ],
    "verbose": true
